generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UserProfile {
  id         String   @id @default(uuid())
  cognitoSub String   @unique @map("cognito_sub")
  firstName  String?  @map("first_name")
  lastName   String?  @map("last_name")
  createdAt  DateTime @default(now()) @map("created_at")

  @@map("user_profiles")
}
